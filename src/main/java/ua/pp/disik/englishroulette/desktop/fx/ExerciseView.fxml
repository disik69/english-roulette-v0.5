<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="ua.pp.disik.englishroulette.desktop.fx.ExercisePresenter"
          hgap="5" vgap="5"
          gridLinesVisible="true" style="-fx-background-color: red;" >
        <padding>
            <Insets bottom="5" left="5" right="5" top="5" />
        </padding>
        <columnConstraints>
            <ColumnConstraints percentWidth="50"/>
            <ColumnConstraints percentWidth="50"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints percentHeight="10"/>
            <RowConstraints percentHeight="90"/>
        </rowConstraints>
        <children>
            <Label text="Foreign"
                   GridPane.columnIndex="0"
                   GridPane.rowIndex="0" />
            <Label text="Native"
                   GridPane.columnIndex="1"
                   GridPane.rowIndex="0" />
            <ListView fx:id="foreignList"
                   GridPane.columnIndex="0"
                   GridPane.rowIndex="1" />
            <ListView fx:id="nativeList"
                   GridPane.columnIndex="1"
                   GridPane.rowIndex="1" />
        </children>
</GridPane>
